{
  "name": "simpleGestures",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This is simple mouse gestures extension.",
  "permissions": [
      "tabs",
      "storage"
  ],
  "background": {
    "scripts": [
      "js/lib_option.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/jquery-1.11.2.min.js",
        "js/content_scripts.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "options_page": "options_page.html"
}