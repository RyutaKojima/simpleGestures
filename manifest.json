{
	"name": "simpleGestures",
	"version": "0.0.9",
	"manifest_version": 2,
	"description": "simple mouse gestures.",
	"icons": {
		"16": "img/SG16.png",
		"48": "img/SG48.png",
		"128": "img/SG128.png"
	},
	"permissions": [
		"tabs",
		"storage"
	],
	"background": {
		"scripts": [
			"js/lib_option.js",
			"js/background.js"
		]
	},
	"content_scripts": [{
		"matches": [
			"<all_urls>"
		],
		"js": [
			"js/external/jquery-1.11.3.min.js",
			"js/lib_gesture.js",
			"js/content_scripts.js"
		],
		"all_frames": true,
		"run_at": "document_start"
	}],
	"options_page": "html/options_page.html"
}