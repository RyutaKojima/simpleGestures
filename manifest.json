{
	"name": "simpleGestures",
	"version": "1.1.0",
	"manifest_version": 2,
	"description": "simple mouse gestures.",
	"icons": {
		"16": "img/SG16.png",
		"48": "img/SG48.png",
		"128": "img/SG128.png"
	},
	"permissions": [
		"sessions",
		"tabs",
		"storage"
	],
	"background": {
		"scripts": [
			"js/app/debug_flg.js",
			"js/app/mouse.js",
			"js/app/keyboard.js",
			"js/app/lib_option.js",
			"js/app/content/lib_gesture.js",
			"js/app/background/background.js"
		]
	},
	"content_scripts": [{
		"matches": [
			"<all_urls>"
		],
		"css": [
			"css/font/flaticon.css"
		],
		"js": [
			"js/vendor/jquery-1.11.3.min.js",
			"js/app/debug_flg.js",
			"js/app/mouse.js",
			"js/app/keyboard.js",
			"js/app/lib_option.js",
			"js/app/content/lib_gesture.js",
			"js/app/content/trail_canvas.js",
			"js/app/content/content_scripts.js",
			"js/app/content/handler.js"
		],
		"all_frames": true,
		"run_at": "document_start"
	}],
	"web_accessible_resources": [
		"css/font/*"
	],
	"options_page": "html/options_page.html"
}